BKend2.php 

 

<?php 

$servername="localhost"; 

$username="Rinku123"; 

$password="rinku@2612"; 

$databasename="register"; 

$Fname=$_POST['fname']; 

$Lname=$_POST['lname']; 

$Uname=$_POST['Uname']; 

$Pass=$_POST['pass']; 

$CPass=$_POST['cpass']; 

  

$conn=new mysqli($servername,$username,$password,$databasename); 

if($conn->connect_error){ 

die("connection_failed".$conn->connection_error); 

} 

$sql = "INSERT INTO registration_details (User_fname, User_lname, User_mail,User_pass,User_Cpass) 

VALUES ('$Fname', '$Lname', '$Uname','$Pass','$CPass')"; 

if($Pass==$CPass){ 

if ($conn->query($sql) === TRUE) { 

  echo "New record created successfully"; 

  header("Location:Homepage.html"); 

} else { 

  echo "Error: " . $sql . "<br>" . $conn->error; 

} 

} 

else 

{ 

echo "Passwords does not match. Enter agian"; 

header("location:Register.html"); 

} 

  

$conn->close(); 

?> 

 

BKend3.php 

<?php 

$servername="localhost"; 

$username="Rinku123"; 

$password="rinku@2612"; 

$databasename="register"; 

$Username=$_POST['Uname']; 

$Password=$_POST['pass']; 

$num=1; 

$admin='admin@email.com'; 

$pass='sweety'; 

  

$conn= new mysqli($servername,$username,$password,$databasename); 

if($conn->connect_error){ 

die("Connection failed".$conn->connect_error); 

} 

$sql="SELECT User_Id,User_mail,User_pass FROM registration_details"; 

// $result=$conn->query($sql); 

if($result=$conn->query($sql)){ 

while ($row=$result->fetch_assoc()) { 

if($Username==$admin){ 

if ($Password==$pass) { 

header("Location:Admin.html"); 

} 

} 

else{ 

 if($Username==$row["User_mail"]){ 

if($Password==$row["User_pass"]){ 

echo '<script type="text/javascript"> 

window.alert("Logged in succesfully"); 

</script>'; 

header("Location:HomePage.html"); 

} 

else{ 

echo "<script type=javascript> 

prompt(Password is wrong); 

</script>"; 

header("Location:Login.html"); 

} 

} 

else{ 

echo "Username is wrong"; 

header("Location:Login.html"); 

} 

} 

} 

} 

else{ 

echo "Error"; 

} 

$conn->close(); 

?> 

 

 

